			$(function() {
				var email_required_tip = "Email address is required.";
				var email_format_tip = "Enter valid email address.(eg:someone@gmail.com)";
				var pwd_required_tip = "Password is required.";
				var validate_required_tip = "Please enter the characters in the image.";
				var hide_str = "hide";
				var show_str = "show";
				var redirect_url = "https://www.fs.com/login.html";
				$('.lr_content_password_show').click(function() {

					var _this = $(this);
					_this.toggleClass('active');
					if (_this.hasClass('active')) {
						_this.html(hide_str);
						$('#password_login').prop('type', 'text');
					} else {
						$(this).html(show_str);
						$('#password_login').prop('type', 'password');
					}
				});
				$('.email_Delete_popup').click(function() {
					window.location.href = redirect_url;
					layer.closeAll();
				});

				//按钮交互效果
				$('#login_submit').click(function(event) {
					var passwd = $('#password_login').val();
					event.preventDefault();
					if (!passwd || passwd.length < 5) {
						$('#password_login').siblings('.error_prompt').show().text(pwd_required_tip) return;
					} else {
						$('#password_login').siblings('.error_prompt').hide();
					}
					//加密
					var MODULUS = 'A4C63A7DA502500A16C6FF509718578CE7874EB50D0E964EE3C7B321422DBE7182D8D21B108E5DE1FB40A145F8B3793B39F092EC2D0907EC0881E02A84DE81FEA676035F57EBEA1F9DD997B1218B5DA699FF41EAD3F1AC5976651379BB0251C65124A97BAE0F1D13B29D41FFFF7038BBDE03257BF4B1338BF361E56C5FDAE6C7';
					var email = $("#email_address_login").val();
					var password = $("#password_login").val();
					var rsa = new RSAKey();
					var modulus = MODULUS;
					var exponent = "10001";
					var text = $(this).text();
					var serect_code = "8b82fdf7a21c5c133a08c202554dbfc4";
					rsa.setPublic(modulus, exponent);
					password = rsa.encrypt(password);
					$("#password_login_encrypt").val(password);
					var img = $('<div id="loader_order_alone" class="loader_order"><svg class="circular" viewBox="25 25 50 50"><circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="4" stroke-miterlimit="10"></circle></svg></div>');
					$(this).attr('disabled', 'disabled').addClass('cart_a_one alone_active').html(img);
					var url = "index.php?main_page=login&action=process";
					var _this = $(this);
					var form_data = {
						'email': email,
						'password': password,
						'securityToken': serect_code,
						// 'password_login':$("#password_login").val(),
						'region': 'login_expire',
					};
					if ($('#validate_div').hasClass('active')) {
						form_data['validate'] = $.trim($('#validate').val());
						form_data['validate_visible'] = 1;
					} else {
						form_data['validate'] = '';
						form_data['validate_visible'] = 0;
					}
					$.ajax({
						url: url + "&ajax_request_action=login",
						data: form_data,
						type: "POST",
						dataType: "json",
						success: function(data) {
							if (data.status == 1) {
								window.location.reload();
							} else {
								if (data.data.is_show_ver) { // 是否展示验证码
									$('.lr-character-group').addClass('active');
								}
								_this.removeAttr('disabled').removeClass('cart_a_one alone_active').html(text);
								$("#fiberstore_message").show().html(data.info);
							}
						}
					})
				})
			})
		</script>
