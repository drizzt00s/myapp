<script>
	function Close_Popup_account() {
		//break修改的弹窗背景
		$('#zConfirm2').hide();

		$('#popup,#window').hide();
		$('.Fiberstore_sel_box').removeClass('active');
		$('.addr_alert').removeClass('active');
	}
	var shpping_addr_id; // shipping address id
	var edit_addr_type; // edit shipping address or billing address

	function edit_address_acc(o,edit_type){
		//edit_type 1 : shipping address
		//edit_type 1 : billing address
		$(".web_popup").show();
		shpping_addr_id = $.trim($(o).parents("div.account-wrap").find(".shippingAdd_id").text());
		edit_addr_type = edit_type;
	}




	function del_shipping_address(o, address_type){
		//address_type is not used. this function only to delete shipping address
		var addressId = $.trim($(o).parents("div.account-wrap").find(".shippingAdd_id").text());
		alert(addressId);
		$.ajax({
			url: "/del_shipping_address",
			dataType: "json",
			type:"post",
			data:{
				addressId:addressId,
				address_type:address_type
			},
			success: function (d) {
				if(d.code == 1){
					alert("This shipping address is deleted");
					window.location.href = window.location.href;
				}
			}
		});
	}
	function del_billing_address(o){
		var addressId = $.trim($(o).parents("div.account-wrap").find(".shippingAdd_id").text());

		$.ajax({
			url: "/del_billing_address",
			dataType: "json",
			type:"post",
			data:{
				addressId:addressId
			},
			success: function (d) {
				if(d.code == 1){
					alert("This billing address is deleted");
					window.location.href = window.location.href;
				}
			}
		});
	}
</script>
<script>
$(document).ready(function(){


	$(".account-wrap").find("*").bind("click", function(e){
		$(e.target).parents("div.account-wrap").addClass('selected');
		$(e.target).parents("div.account-wrap").siblings("div.account-wrap").removeClass("selected");
		$(e.target).parents("div.account-wrap").find(".Fiberstore_edt_btn").show();
		$(e.target).parents("div.account-wrap").siblings("div.account-wrap").find(".Fiberstore_edt_btn").hide();


	});

	$(".account-wrap").bind("click", function(e){
		if(e.target == this){
			$(e.target).addClass('selected');
			$(e.target).siblings("div.account-wrap").removeClass("selected");
			$(e.target).find(".Fiberstore_edt_btn").show();
			$(e.target).siblings("div.account-wrap").find(".Fiberstore_edt_btn").hide();
		}
	});
})
