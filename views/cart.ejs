                var proicon01 = "&#xf022;";
                var proicon02 = "&#xf431;";
                var proicon = '';
                var eachicLen = $(".shipment-shopcart-checkpro-eachic").length;
                var num = [],
                    pid = [],
                    is_checked = [];
                if (!$(th).hasClass("choosez")) {
                    proicon = proicon02;
                    $(th).addClass("choosez");
                    if ($(th).hasClass('shipment-shopcart-checkpro-allmain')) {
                        $(".shipment-shopcart-checkpro-eachicbox").addClass("choosez");
                    }
                } else {
                    proicon = proicon01;
                    $(th).removeClass("choosez");
                    if ($(th).hasClass('shipment-shopcart-checkpro-allmain')) {
                        $(".shipment-shopcart-checkpro-eachicbox").removeClass("choosez");
                    }
                }
                if (!$(th).hasClass('shipment-shopcart-checkpro-allmain')) {
                    $(th).find(".shipment-shopcart-checkpro-eachic").html(proicon);
                } else {
                    $(th).find(".icon").html(proicon);
                    $(".shipment-shopcart-checkpro-eachic").html(proicon);
                }
                var eachic_activeLen = $(".shipment-shopcart-checkpro-eachicbox.choosez").length;
                var cantcheck_len = $(".checkpro-eachicbox-cantcheck").length;
                if (eachic_activeLen >= eachicLen - cantcheck_len) {
                    $(".shipment-shopcart-checkpro-allmain").addClass("choosez").find(".icon").html(proicon02);
                } else {
                    $(".shipment-shopcart-checkpro-allmain").removeClass("choosez").find(".icon").html(proicon01);
                }
                if ($(th).hasClass('shipment-shopcart-checkpro-allmain')) {
                    $(".checkpro-eachicbox-cantcheck").removeClass("choosez").find(".shipment-shopcart-checkpro-eachic").html(proicon01);
                }

                if ($('.shipment-shopcart-checkpro-eachicbox').length > 0) {
                    $('.shipment-shopcart-checkpro-eachicbox').each(function() {
                        var productId = $(this).parents('.shipment-shopcart-list').attr('data-cartid');
                        var productQty = $(this).parents('.shipment-shopcart-list').find('input[name^="cart_quantity"]').val();
                        if (productQty) {
