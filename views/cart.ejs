<script type='text/javascript'>
    var cartTotal = 0;
    // cart total money
    var pdSubTotal = 0;
    // each product's current total moneny

    var pdQty = 0;
    // how many products in cart

    function confirm_checkout(){
        //check if any of cart pd is selected
        //check selected pd's pid
        var pid = []; // all products waiting to be checked out
        $(".cartCheck").each(function (i, v) {
            if(v.checked){
                var selectedPid = $(v).parent("span").next().attr("pid");
                pid.push(selectedPid);
            }
        });
        if(pid.length <= 0){
            alert("at least one product must be selected");
            return false;
        }else{
            $.ajax({
                url: "/confirm_checkout_data",
                dataType: "json",
                type:"post",
                data:{
                    pid:pid
                },
                success: function (d) {
                    if(d.code == 1){
                        window.location.href = "/confirm_order";
                    }
                }
            });
        }
    }

    function toggleSelectAll(o){
        //get cart total money, when all checked. set cartTotalVal
        var checkAllStatus = o.checked;
        if(checkAllStatus){
            $(".cartCheck").each(function (i, v) {
                if(!v.checked){
                    v.checked = true;
                }
            });
            $("#total_js_s").text(cartTotal);
            $("#cartQty").text(pdQty);
        }else{
            $(".cartCheck").each(function (i, v) {
                if(v.checked){
                    v.checked = false;
                }
            });
            $("#total_js_s").text("0.00");
            $("#cartQty").text("0");
            //when all checked. set cartTotal moeny to 0
        }
    }

    function changeStausAll() {
        var isAllchekced = true;
        $(".cartCheck").each(function (i, v) {
            if(!v.checked){
                isAllchekced = false
            }
        });
        if(isAllchekced){
            $("#toggleSelectAll")[0].checked = true;
        }else{
            $("#toggleSelectAll")[0].checked = false;
        }
    }


    function disableCart(o) {
        pdSubTotal = parseInt($(o).parents("div.new-shopcart-tableBox").find("span.pdSubTotal").text());
        setTimeout(changeStausAll, 0);
        // alert(o.checked);
        if(o.checked){
            var currentCartTotalVal = parseInt($.trim($("#total_js_s").text()));
            $("#total_js_s").text(currentCartTotalVal + pdSubTotal);
            var currentCartLen = parseInt($.trim($("#cartQty").text()));
            $("#cartQty").text(currentCartLen + 1);

        }else{
            var currentCartTotalVal = parseInt($.trim($("#total_js_s").text()));
            $("#total_js_s").text(currentCartTotalVal - pdSubTotal);
            var currentCartLen = parseInt($.trim($("#cartQty").text()));
            $("#cartQty").text(currentCartLen - 1);
        }
    }
</script>
<script>
    $(document).ready(function(){


        utility_fte.initHeaderBav();


        function getCartTotalVal(){
            //get cart total money
            var cartTotalVal = parseInt($.trim($("#total_js_s").text()));
            cartTotal = cartTotalVal;
        }
        getCartTotalVal();

        function getCartQty(){
            var cartLen = parseInt($.trim($("#cartQty").text()));
            pdQty = cartLen;
        }
        getCartQty();



        function checkAllPd() {
            //check all pd checkbox after entering
            $(".cartCheck").each(function (i, v) {
                v.checked = true;
            });
            document.getElementById("toggleSelectAll").checked = true;
        }
        checkAllPd();


        $(".delCartPd").bind("click", function (e) {
            var pid = parseInt($.trim($(e.target).parents(".new-shopcart-tableBox-parent").find(".pdPid").text()));

            $.ajax({
                url: "/del_cart_pd",
                dataType: "json",
                type:"post",
                data:{
                    pid:pid,
                },
                success: function (d) {
                    if(d.code == 1){
                        alert(d.data);
                        window.location.href = window.location.href;
                    }
                }
            });
        });

        function cartQtyUpdate(currentVal, pid){
            $.ajax({
                url: "/update_each_cart_qty",
                dataType: "json",
                type:"post",
                data:{
                    updatedQty:currentVal,
                    pid:pid
                },
                success: function (d) {
                    if(d.code == 1){
                        window.location.href = window.location.href;
                    }
                }
            });
        }


        ($(".cart_qty_add")).bind("click",function(e){
            var currentVal = parseInt($(e.target).parents("div").prev().val()) + 1;
            var pid =  parseInt($(e.target).parents("div.pdWrap").find(".pdPid").text());
            $(e.target).parents("div").prev().val(currentVal);
            cartQtyUpdate(currentVal, pid);
        });

        $(".cart_qty_reduce").bind("click",function (e) {
            var currentVal =  parseInt($(e.target).parents("div").prev().val()) - 1;
            if(currentVal == 0){
                return false;
            }
            var pid =  parseInt($(e.target).parents("div.pdWrap").find(".pdPid").text());
            $(e.target).parents("div").prev().val(currentVal);
            cartQtyUpdate(currentVal, pid);
        });

    })
</script>

