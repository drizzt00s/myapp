  <script type='text/javascript'>
$(document).ready(function(){


  function getProfile(){
    $.ajax({
      url: "/user_profile",
      dataType: "json",
      success: function (d) {
        if(d.code == 0){
          alert("用户未登录");
        }else if(d.code == 1){
          var fullname = d.d;
          $("#topHello").text(fullname);
        }
      }
    });
  }
  getProfile();
  
  $(".navSelect").mouseover(function(e){
    e.stopPropagation();
    var index = $(e.target).attr("navSelect");
    if(index == undefined){
      return;
    }
    if(index == 1){
      $.each($(".subGpdListLi").find(".parentID"), function(i, v){
        if($(v).text() == 1){
          $(v).parent("li").removeClass("myHidden");
        }
      });
    }
  });

  $("li.gpdListLi").mouseover(function(e){
    e.stopPropagation();
    var proID = $(e.target).find("span.gpdListTag").text();
    if(proID == 0){
      return;
    }
    $.each($(".subGpdListLi").find(".parentID"), function(i,v){
      if($(v).text() == proID){
        $(v).parent("li").removeClass("myHidden");
      }else{
        $(v).parent("li").addClass("myHidden");
      }
    });
    
    // console.log(proID);
  });
});
  </script>
  


