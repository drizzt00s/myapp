  <script type='text/javascript'>
$(document).ready(function(){
    // const socket = io.connect('http://localhost:3001');
    // socket.on("connection");
  function getProfile(){
    $.ajax({
      url: "/user_profile",
      dataType: "json",
      success: function (d) {
        if(d.code == 0){
          // alert("用户未登录");
        }else if(d.code == 1){
          var fullname = d.d;
          $("#topHello").text(fullname);
        }
      }
    });
  }
  getProfile();
  utility_fte.initHeaderBav();

  $("#askQuote").bind("click", function(e){
    var quote_user_name = $.trim($("#quote_user_name").val());
    var quote_user_mail = $.trim($("#quote_user_mail").val());
    var quote_contents = $.trim($("#quote_contents").val());
    if(!quote_user_name){
      alert("Please enter your name");
      return false;
    }
    if(!quote_user_mail){
      alert("Please enter your mail");
      return false;
    }
    if(!quote_contents){
      alert("Please enter your quote contents");
      return false;
    }

    var quote_contents = {
      name:quote_user_name,
      mail:quote_user_mail,
      contents:quote_contents
    }
    $.ajax({
      url: "/ask_quote",
      dataType: "json",
      data:{
        contents:quote_contents
      },
      type:"POST",
      success: function (d) {
        if(d.code == 1){
          alert("We will contact you soon.");
        }
      }
    });
  });
});
  </script>
