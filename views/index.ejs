    //产品滑动
    var fs_home_nav_content_li_swiper;
    var fs_home_nav_content_ul_swiper;
    if ($(window).width() < 960) {
        fs_home_nav_content_li_swiper = new Swiper('.fs_home_nav_ul_overflow .swiper-container', {
            paginationClickable: true,
            spaceBetween: 40,
            slidesPerView: 'auto',
            breakpoints: {
                960: {
                    spaceBetween: 0,
                },
