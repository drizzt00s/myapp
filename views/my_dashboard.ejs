            $('.fs_account_tab_ul li').click(function() {
                var status = 1;
                if ($(this).hasClass("active")) {
                    status = 0;
                }
                $(this).addClass('active').siblings('li').removeClass('active');
                var width = ($(this).width() - 40) / 2;
                var left = ($(this).offset().left - $('.fs_account_tab_ul_overflow').offset().left + width) + 'px';
                $('.home_compati').css('left', left);

                var type = $(this).attr("tag");
                if (status) {
                    showMoreOrders(type, 1);
                }
            })

            if ($(window).width() > 960) {
                var fs_this = $('.fs_account_tab_ul li').eq(0);
                var fs_account_width = (fs_this.width() - 40) / 2;
                var fs_account_left = (fs_this.offset().left - $('.fs_account_tab_ul_overflow').offset().left + fs_account_width) + 'px';
                $('.home_compati').css('left', fs_account_left);
            }

            function beforeSubmit(form) {
                if (form.search.value == '') {
                    form.search.focus();
                    return false;
                }
                return true;
            }

            $(document).on('click', '.close_account',
                function() {
                    var customer_id = "297215";
                    $.cookie('close_account', customer_id, {
                        expires: 365
                    });
                    $('#consumption_tax').hide();
                });

            $(document).on('click', '#pending_order_msg',
                function() {
                    var customer_id = "297215";
                    $.cookie('close_pending_order_msg', customer_id, {
                        expires: 365
                    });
                    $(this).parent(".fs_public_warnings").hide();
                });
            var wirte_a_review = 'Leave Feedback about FS';
            var https = 'https://www.fs.com';

            var base_url = 'https://www.fs.com/index.php?main_page=quotes_list';
            var current_status = 0;
            var current_time = 0;
            var quotes_number = '';
            var page = 1;
            $(document).on('click', '.search_number',
                function() {
                    quotes_number = $('#number_input').val();
                    search();
                });
            $(function() {
                $('#number_input').bind('keypress',
                    function(event) {
                        if (event.keyCode == "13") {
                            quotes_number = $('#number_input').val();
                            search();
                        }
                    });
            });

            function search() {
                show_loading($(this), 'show', 5);
                var url_param = '';
                if (quotes_number != '') {
                    url_param += '&quotes_number=' + quotes_number;
                } else {
                    if (current_status != 0) {
                        url_param += '&status=' + current_status;
                    }
                    if (current_time != 0) {
                        url_param += '&time_type=' + current_time;
                    }
                }
                if (page) {
                    url_param += '&page=' + page;
                }
                window.location.href = base_url + url_param;
            }

            function addCart(inquiry_id) {
                var $loading = '<div id="loader_order_alone" class="loader_order"><svg class="circular" viewBox="25 25 50 50"><circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="4" stroke-miterlimit="10"></circle></svg></div>';
                var link_url = "https://www.fs.com/index.php?main_page=checkout" + "&inquiry_id=" + inquiry_id;
                link_url = link_url.replace("amp;", "");
                $("#public_loading_two_" + inquiry_id).css('display', 'inline-block');
                $("#public_loading_two_" + inquiry_id).append($loading);
                $("#public_loading_two_" + inquiry_id).addClass('active');
                $("#public_loading_two_" + inquiry_id).find('.span').css('visibility', 'hidden');
                $.ajax({
                    type: "POST",
                    dataType: "json",
                    url: "index.php?main_page=inquiry_detail&action=reorder",
                    data: {
                        "inquiry_id": inquiry_id
                    },
                    success: function(data) {
                        if (data.status == 1) {
                            window.location.href = link_url;
                        } else {
                            alert(data.info);
                        }
                    }
                });
            }

            /**
             *  quotes again
             * @param quotes_id
             * @param status
             * @param _this
             */
            function quote_again(quotes_id, type, status, _this) {
                var _this = $(_this);
                var formData = '';
                if (status == 3) {
                    formData = $("#buyAgainForm").serialize();
                }
                $.ajax({
                    url: '?modules=ajax&handler=ajax_quotes_request&ajax_request_action=quotes_again',
                    data: formData + '&quotes_id=' + quotes_id + '&type=' + type + '&status=' + status,
                    type: 'post',
                    dataType: 'json',
                    beforeSend: function() {
                        loading(_this);
                    },
                    success: function(msg) {
                        unLoading(_this);
                        if (msg.data.result) {
                            if (status == 1 || status == 2) {
                                var html = msg.data.html;
                                if (status == 1) {
                                    html = html.replace('onclick="buy_again(', 'onclick="quote_again(');
                                }
                                $("#orders_buy_again .alert_popup_content").html(html);
                                $("#orders_buy_again").show();
                            } else {
                                cancel_buy_again();
                                $('#product_cart_popup').html(msg.data.html);
                                $('.new_product_popup_addCart').show();
                                //更新头部购物车板块
                                if (msg.data.isMobile) {
                                    $(".m_cart").html(msg.data.topCartHtml);
                                } else {
                                    $("#ShoppingCartInfo").html(msg.data.topCartHtml);
                                }
                            }
                        } else {
                            show_server_tip_window({
                                type: 'error',
                                content: system_busy_tip,
                            });
                        }
                    }

                })
            }

            $('.new-customer-tips-btn a').click(function(event) {
                $(this).parents('.new-customer-tips').remove();
                event.stopPropagation();

