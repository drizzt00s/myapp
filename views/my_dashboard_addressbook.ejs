                commonButtonLoading('#order_submit','off','提交订单');
                return false
            }
        }
        $.ajax({
            type: "POST",
            url: "ajax_process_other_requests.php?request_type=create_order",
            data:{'securityToken':'446ece1dc015ab106d30c054bba93bb6','remark':remark,'receipt_key':receipt_key,'receipt_value':receipt_value},
            dataType: "json",
            beforeSend: function(){
                $("#order_submit").attr('data-is-submit',1);
                commonButtonLoading('#order_submit','on','');
            },
            success: function(data){
                if(data.error === 'err'){
                    commonButtonLoading('#order_submit','off','提交订单');
                    zAlert({
                        title:'提示',
                        msg:data.message,
                        btn:'',
                        cb:function(){
                            location.href="https://cn.fs.com/checkout.html";
                        }
                    });
                    return false;
                }else{
                    setTimeout(function() {
                        window.location.href="https://cn.fs.com/index.php?main_page=order_submit&rid="+data;
