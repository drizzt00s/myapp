<script>

    function Close_Popup_account() {
        //break修改的弹窗背景
        $('#zConfirm2').hide();

        $('#popup,#window').hide();
        $('.Fiberstore_sel_box').removeClass('active');
        $('.addr_alert').removeClass('active');
    }

    var shpping_addr_id; // shipping address id
    var edit_addr_type; // edit shipping address or billing address
    function edit_address_acc(o,edit_type){
        //edit_type 1 : shipping address
        //edit_type 1 : billing address
        $(".web_popup").show();
        shpping_addr_id = $.trim($(o).parents("div.account-wrap").find(".shippingAdd_id").text());
        edit_addr_type = edit_type;
    }

    function del_billing_address(o){
        var addressId = $.trim($(o).parents("div.account-wrap").find(".shippingAdd_id").text());
        $.ajax({
            url: "/del_billing_address",
            dataType: "json",
            type:"post",
            data:{
                addressId:addressId
            },
            success: function (d) {
                if(d.code == 1){
                    alert("This billing address is deleted");
                    window.location.href = window.location.href;
                }
            }
        });
    }

    function del_shipping_address(o, address_type){
        //address_type is not used. this function only to delete shipping address
        var addressId = $.trim($(o).parents("div.account-wrap").find(".shippingAdd_id").text());
        alert(addressId);
        $.ajax({
            url: "/del_shipping_address",
            dataType: "json",
            type:"post",
            data:{
                addressId:addressId,
                address_type:address_type
            },
            success: function (d) {
                if(d.code == 1){
                    alert("This shipping address is deleted");
                    window.location.href = window.location.href;
                }
            }
        });
    }



    function set_default_address(o,address_type){
        // address_type:1 shipping address
        // address_type:2 billing address
        var addressId = $.trim($(o).parents("div.account-wrap").find(".shippingAdd_id").text());
        $.ajax({
            url: "/set_default_address",
            dataType: "json",
            type:"post",
            data:{
                addressId:addressId,
                address_type:address_type
            },
            success: function (d) {
                if(d.code == 1){
                    alert("This is set to your default address.");
                    $(o).parents("div.account-wrap").find(".defaultAdd_att").show();
                    window.location.href = window.location.href;
                }
            }
        });
    }
</script>

