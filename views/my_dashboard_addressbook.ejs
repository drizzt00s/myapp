<script type="text/javascript">
    var add_num = 3;
    //判断是否为繁文的站点这里的方式不同
    var is_fan = '';
    // 提交订单
    $("#order_submit").click(function(){
        var remark = $('.remark').val();
        var checkDom = $('.checkout_address').find('div').hasClass('checkout_address_02');
        //获取是否为繁文站站的状态
        var is_fan_site = fan_site(is_fan);
        var receipt = CheckInvoice();
        var is_submit = $("#order_submit").attr('data-is-submit');
        if (is_submit == 1){
            return false;
        }
        if (!is_fan_site){
            if (receipt){
                var receipt_key = receipt[0];
                var receipt_value = receipt[1];
            }else {
                var type = $(".invoiceInfor_tabList").find('li.active').attr('invoice_type');
                if (type === 'invoice_valAddtax') {
                    $(".next-step-tips").addClass("active");
                    for(var i = 0; i<= $(".invoiceInfor_formList03 li").length - 1;i ++){
                        if($(".invoiceInfor_formList03[step_process='step_second'] li").eq(i).find(".invoice-error").hasClass("active") && $("#invoice_valAddtax").hasClass("active02")){
                            $(".next-step-tips").removeClass("active");
                        }
                    }
                    if($("#invoice_valAddtax").hasClass("active03")){
                        $(".next-step-tips").removeClass("active");
                    }
                }
                if($(".invoice-error.active").parents(".invoiceInfor_inputBox").hasClass("invoiceInfor_moreOptions")){
                    $(".invoice-error.active").parents(".invoiceInfor_inputBox").addClass("active");
                    $(".invoice-error.active").parents(".invoiceInfor_moreMain").slideDown(300);
                };
                $("html,body").animate({ scrollTop: $(".invoiceInfor_box").offset().top}, 300);
                return false;
            }
