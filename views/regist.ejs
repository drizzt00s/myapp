        /* WinBox Theme: "custom" */ .winbox.custom { background: linear-gradient(90deg,
        #ff00f0, #0050ff); box-shadow: none; border-radius: 12px 12px 0 0; } .winbox.custom.max,
        .winbox.custom.min { border-radius: 0; } .winbox.custom .wb-body { /* margin
        represents the width of window border: */ margin: 4px; color: #fff; background:
        #131820; } .winbox.custom .wb-title { font-size: 13px; text-transform:
        uppercase; font-weight: 600; } .winbox.custom .wb-icon * { opacity: 0.65;
        } .winbox.custom .wb-icon *:hover { opacity: 1; } .winbox.custom .wb-max
