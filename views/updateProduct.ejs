<script>
    $(document).ready(function (e) {

        // let totalSubPdMenu = [];

        $("#pd1").bind("change",switchSubPdMenu);
        
        function switchSubPdMenu(e) {
            let SubPdMenuBoundaryFt;
            let SubPdMenuBoundaryEd;
            let index = e.target.selectedIndex;
            switch (index){
                case 0:
                    SubPdMenuBoundaryEd = 0;
                    SubPdMenuBoundaryFt = 5;
                    break;
                case 1:
                    SubPdMenuBoundaryEd = 6;
                    SubPdMenuBoundaryFt = 14;
                    break;
                case 2:
                    SubPdMenuBoundaryEd = 15;
                    SubPdMenuBoundaryFt = 20;
                    break;
                case 3:
                    SubPdMenuBoundaryEd = 21;
                    SubPdMenuBoundaryFt = 32;
                    break;
                case 4:
                    SubPdMenuBoundaryEd = 33;
                    SubPdMenuBoundaryFt = 37;
                    break;
                case 5:
                    SubPdMenuBoundaryEd = 38;
                    SubPdMenuBoundaryFt = 40;
                    break;
                case 6:
                    SubPdMenuBoundaryEd = 41;
                    SubPdMenuBoundaryFt = 46;
                    break;
                case 7:
                    SubPdMenuBoundaryEd = 47;
                    SubPdMenuBoundaryFt = 52;
                    break;
            };


            $(".pd2Opt").each(function(i, v){
                if(i >= SubPdMenuBoundaryEd && i <= SubPdMenuBoundaryFt){
                    $(v).show();
                    $("#pd2").val('');
                } else{
                    $(v).hide();
                }
            });
        }


        // function getTotalSubPdMenu(){
        //      $(".pd2Opt").each(function (i, v) {
        //          totalSubPdMenu.push(v);
        //      });
        //
        // }
        // getTotalSubPdMenu();

        function initSubPdMenu(){
            $(".pd2Opt").each(function(i, v){
                if(i > 5){
                    $(v).hide();
                }
            });
        }
        initSubPdMenu();

        $("#submit").bind("click",function (e){
            let newPdName = $("#newPdName").val();
            let newPdInfo = $("#newPdInfo").val();
            let newPdDes = $("#newPdDes").val();
            let newPdFeature = $("#newPdFeature").val();
            // these four value must not be null;
            if(!newPdName){
                alert("产品名称不能为空");
            }
            if(!newPdInfo){
                alert("产品基本信息不能为空");
            }
            if(!newPdDes){
                alert("产品描述不能为空");
            }
            if(!newPdFeature){
                alert("产品特色不能为空");
            }
            document.getElementById("newPdSubmit").submit();
        });
    })
</script>
